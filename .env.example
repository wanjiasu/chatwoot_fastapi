# Chatwoot base URL, e.g., https://app.chatwoot.com or your self-hosted domain
CHATWOOT_BASE_URL=https://app.chatwoot.com
# Generate from Chatwoot profile -> Access Token
CHATWOOT_API_TOKEN=your_api_access_token_here

# Optional: Restrict webhook handling to a specific inbox (Chatwoot inbox_id)
# Set this to the Telegram inbox id you want to handle
TELE_STOCKTRADE_INBOX_ID=123
